{
  "env":
  {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions":
  {
    "ecmaFeatures":
    {
      "experimentalObjectRestSpread": true,
      "jsx": true,
      "modules": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules":
  {
    "indent": [
      2,
      2
    ],
    "no-console": 1,
    "react/no-set-state": "error",
    "linebreak-style": [
      2,
      "unix"
    ],
    "quotes": [
      2,
      "double"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-unused-vars": [
      1,
      {
        "vars": "all",
        "args": "after-used"
      }
    ]
  }
}